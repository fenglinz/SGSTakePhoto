    <sys:String x:Key="User">用户</sys:String>
    <sys:String x:Key="Organization">组织</sys:String>
    <sys:String x:Key="System">执行系统</sys:String>

    <!--文本框-->
    <sys:String x:Key="DefaultWidth">图片默认宽度</sys:String>
    <sys:String x:Key="DefaultHeight">图片默认宽高度</sys:String>
    <sys:String x:Key="DefaultDPI">图片默认像素密度</sys:String>
    <sys:String x:Key="Notice">点击保存后生效</sys:String>
</ResourceDictionary>